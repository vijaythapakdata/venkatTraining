{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "sample-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sampleForm/SampleFormWebPart.js",
          "manifest": "./src/webparts/sampleForm/SampleFormWebPart.manifest.json"
        }
      ]
    },
    "file-picker-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/filePicker/FilePickerWebPart.js",
          "manifest": "./src/webparts/filePicker/FilePickerWebPart.manifest.json"
        }
      ]
    },
    "large-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/largeList/LargeListWebPart.js",
          "manifest": "./src/webparts/largeList/LargeListWebPart.manifest.json"
        }
      ]
    },
    "formik-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/formikForm/FormikFormWebPart.js",
          "manifest": "./src/webparts/formikForm/FormikFormWebPart.manifest.json"
        }
      ]
    },
    "hello-world-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/helloWorld/HelloWorldWebPart.js",
          "manifest": "./src/webparts/helloWorld/HelloWorldWebPart.manifest.json"
        }
      ]
    },
    "functional-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/functionalForm/FunctionalFormWebPart.js",
          "manifest": "./src/webparts/functionalForm/FunctionalFormWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SampleFormWebPartStrings": "lib/webparts/sampleForm/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "FilePickerWebPartStrings": "lib/webparts/filePicker/loc/{locale}.js",
    "LargeListWebPartStrings": "lib/webparts/largeList/loc/{locale}.js",
    "FormikFormWebPartStrings": "lib/webparts/formikForm/loc/{locale}.js",
    "HelloWorldWebPartStrings": "lib/webparts/helloWorld/loc/{locale}.js",
    "FunctionalFormWebPartStrings": "lib/webparts/functionalForm/loc/{locale}.js"
  }
}
